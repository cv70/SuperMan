# SuperMan AI Company - Environment Variables
# Copy this file to .env and fill in your values

# OpenAI API Key (required)
OPENAI_API_KEY=sk-...

# Optional LLM Configuration
LLM_MODEL=gpt-4
LLM_TEMPERATURE=0.3
LLM_TIMEOUT=60

# State persistence
STATE_FILE=data/company_state.json

# Logging (optional)
LOG_LEVEL=INFO
